{
  "name": "joes_doughnut_shoppe",
  "version": "1.0.0",
  "description": "Sample integration of Braintree's Hosted Fields with React.js. ",
  "main": "server.js",
  "dependencies": {
    "body-parser": "^1.19.0",
    "braintree": "^2.16.0",
    "concurrently": "^4.1.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "if-env": "^1.0.4",
    "nodemon": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {},
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build"
  },
  "keywords": [],
